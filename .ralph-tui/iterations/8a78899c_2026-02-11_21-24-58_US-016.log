2→// Taps\n    53→// ============================================================\n    54→\n    55→type TapRow = Tap & {\n    56→  beers: Beer | null;\n    57→  tap_pricing: Pick<TapPricing, 'price_12oz'>[];\n    58→};\n    59→\n    60→function computeAvailability(\n    61→  ozRemaining: number,\n    62→  lowThresholdOz: number,\n    63→): TapWithBeer['availability_status'] {\n    64→  if (ozRemaining <= 0) return 'out';\n    65→  if (ozRemaining <= lowThresholdOz) return 'low';\n    66→  return 'available';\n    67→}\n    68→\n    69→function mapTapRow(row: TapRow): TapWithBeer {\n    70→  const pricing = row.tap_pricing?.[0];\n    71→  return {\n    72→    id: row.id,\n    73→    venue_id: row.venue_id,\n    74→    tap_number: row.tap_number,\n    75→    beer_id: row.beer_id,\n    76→    status: row.status,\n    77→    oz_remaining: row.oz_remaining,\n    78→    low_threshold_oz: row.low_threshold_oz,\n    79→    temperature_f: row.temperature_f,\n    80→    temp_ok: row.temp_ok,\n    81→    temp_threshold_f: row.temp_threshold_f,\n    82→    created_at: row.created_at,\n    83→    updated_at: row.updated_at,\n    84→    beer: row.beers ?? null,\n    85→    price_12oz: pricing?.price_12oz ?? null,\n    86→    availability_status: computeAvailability(row.oz_remaining, row.low_threshold_oz),\n    87→  };\n    88→}\n    89→\n    90→/**\n    91→ * Fetch active taps for a venue with joined beer name, style, pricing, and\n    92→ * computed availability_status + temp_ok.\n    93→ */\n    94→export async function fetchVenueTaps(venueId: string): Promise<TapWithBeer[]> {\n    95→  const { data, error } = await supabase\n    96→    .from('taps')\n    97→    .select(\n    98→      `\n    99→      *,\n   100→      beers (*),\n   101→      tap_pricing (price_12oz)\n   102→    `,\n   103→    )\n   104→    .eq('venue_id', venueId)\n   105→    .eq('status', 'active')\n   106→    .order('tap_number');\n   107→\n   108→  if (error) throw error;\n   109→  return (data ?? []).map((row: TapRow) => mapTapRow(row));\n   110→}\n   111→\n   112→// ============================================================\n   113→// Realtime\n   114→// ============================================================\n   115→\n   116→/**\n   117→ * Subscribe to realtime changes on the taps table for a specific venue.\n   118→ * Calls `onUpdate` with the changed tap row whenever an UPDATE occurs.\n   119→ * Returns the RealtimeChannel (call `.unsubscribe()` to clean up).\n   120→ */\n   121→export function subscribeTaps(\n   122→  venueId: string,\n   123→  onUpdate: (tap: Tap) => void,\n   124→): RealtimeChannel {\n   125→  return supabase\n   126→    .channel(`taps:venue_id=eq.${venueId}`)\n   127→    .on<Tap>(\n   128→      'postgres_changes',\n   129→      {\n   130→        event: 'UPDATE',\n   131→        schema: 'public',\n   132→        table: 'taps',\n   133→        filter: `venue_id=eq.${venueId}`,\n   134→      },\n   135→      (payload) => {\n   136→        onUpdate(payload.new);\n   137→      },\n   138→    )\n   139→    .subscribe();\n   140→}\n   141→"}]},"parent_tool_use_id":null,"session_id":"7a792d2b-890a-4566-b438-abfd468cc676","uuid":"8a5590a3-2ed0-4f93-9fb1-135553a6cc3e","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\check\\beerbot\\beerbot-app\\lib\\api\\venues.ts","content":"import { supabase } from '@/lib/supabase';\nimport type {\n  Beer,\n  Tap,\n  TapPricing,\n  TapWithBeer,\n  Venue,\n  VenueWithDistance,\n} from '@/types/api';\nimport type { RealtimeChannel } from '@supabase/supabase-js';\n\n// ============================================================\n// Venues\n// ============================================================\n\n/**\n * Fetch all active venues, optionally sorted by proximity to the given coordinates.\n * When lat/lng are provided, calls the `get_venues_nearby` RPC function which\n * returns results sorted by distance (Haversine) and includes `distance_miles`.\n * Without coordinates, fetches directly from the venues table sorted by name.\n */\nexport async function fetchVenues(params?: {\n  latitude?: number;\n  longitude?: number;\n}): Promise<VenueWithDistance[]> {\n  if (params?.latitude != null && params?.longitude != null) {\n    const { data, error } = await supabase.rpc('get_venues_nearby', {\n      user_lat: params.latitude,\n      user_lng: params.longitude,\n    });\n\n    if (error) throw error;\n    return (data ?? []) as VenueWithDistance[];\n  }\n\n  // No coordinates — simple fetch sorted by name\n  const { data, error } = await supabase\n    .from('venues')\n    .select('*')\n    .eq('is_active', true)\n    .order('name');\n\n  if (error) throw error;\n\n  return (data ?? []).map((v: Venue) => ({\n    ...v,\n    distance_miles: null,\n  }));\n}\n\n// ============================================================\n// Taps\n// ============================================================\n\ntype TapRow = Tap & {\n  beers: Beer | null;\n  tap_pricing: Pick<TapPricing, 'price_12oz'>[];\n};\n\nfunction computeAvailability(\n  ozRemaining: number,\n  lowThresholdOz: number,\n): TapWithBeer['availability_status'] {\n  if (ozRemaining <= 0) return 'out';\n  if (ozRemaining <= lowThresholdOz) return 'low';\n  return 'available';\n}\n\nfunction mapTapRow(row: TapRow): TapWithBeer {\n  const pricing = row.tap_pricing?.[0];\n  return {\n    id: row.id,\n    venue_id: row.venue_id,\n    tap_number: row.tap_number,\n    beer_id: row.beer_id,\n    status: row.status,\n    oz_remaining: row.oz_remaining,\n    low_threshold_oz: row.low_threshold_oz,\n    temperature_f: row.temperature_f,\n    temp_ok: row.temp_ok,\n    temp_threshold_f: row.temp_threshold_f,\n    created_at: row.created_at,\n    updated_at: row.updated_at,\n    beer: row.beers ?? null,\n    price_12oz: pricing?.price_12oz ?? null,\n    availability_status: computeAvailability(row.oz_remaining, row.low_threshold_oz),\n  };\n}\n\n/**\n * Fetch active taps for a venue with joined beer name, style, pricing, and\n * computed availability_status + temp_ok.\n */\nexport async function fetchVenueTaps(venueId: string): Promise<TapWithBeer[]> {\n  const { data, error } = await supabase\n    .from('taps')\n    .select(\n      `\n      *,\n      beers (*),\n      tap_pricing (price_12oz)\n    `,\n    )\n    .eq('venue_id', venueId)\n    .eq('status', 'active')\n    .order('tap_number');\n\n  if (error) throw error;\n  return (data ?? []).map((row: TapRow) => mapTapRow(row));\n}\n\n// ============================================================\n// Realtime\n// ============================================================\n\n/**\n * Subscribe to realtime changes on the taps table for a specific venue.\n * Calls `onUpdate` with the changed tap row whenever an UPDATE occurs.\n * Returns the RealtimeChannel (call `.unsubscribe()` to clean up).\n */\nexport function subscribeTaps(\n  venueId: string,\n  onUpdate: (tap: Tap) => void,\n): RealtimeChannel {\n  return supabase\n    .channel(`taps:venue_id=eq.${venueId}`)\n    .on<Tap>(\n      'postgres_changes',\n      {\n        event: 'UPDATE',\n        schema: 'public',\n        table: 'taps',\n        filter: `venue_id=eq.${venueId}`,\n      },\n      (payload) => {\n        onUpdate(payload.new);\n      },\n    )\n    .subscribe();\n}\n","numLines":141,"startLine":1,"totalLines":141}}}
