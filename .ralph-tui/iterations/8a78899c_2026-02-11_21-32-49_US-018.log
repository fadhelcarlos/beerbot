async function fetchVenue(venueId: string): Promise<Venue> {\n    55→  const { data, error } = await supabase\n    56→    .from('venues')\n    57→    .select('*')\n    58→    .eq('id', venueId)\n    59→    .single();\n    60→\n    61→  if (error) throw error;\n    62→  return data as Venue;\n    63→}\n    64→\n    65→/**\n    66→ * Fetch active tap counts for a list of venue IDs.\n    67→ * Returns a map of venueId -> active tap count.\n    68→ */\n    69→export async function fetchVenueActiveTapCounts(\n    70→  venueIds: string[],\n    71→): Promise<Record<string, number>> {\n    72→  if (venueIds.length === 0) return {};\n    73→\n    74→  const { data, error } = await supabase\n    75→    .from('taps')\n    76→    .select('venue_id')\n    77→    .in('venue_id', venueIds)\n    78→    .eq('status', 'active');\n    79→\n    80→  if (error) throw error;\n    81→\n    82→  const counts: Record<string, number> = {};\n    83→  for (const row of data ?? []) {\n    84→    counts[row.venue_id] = (counts[row.venue_id] ?? 0) + 1;\n    85→  }\n    86→  return counts;\n    87→}\n    88→\n    89→// ============================================================\n    90→// Taps\n    91→// ============================================================\n    92→\n    93→type TapRow = Tap & {\n    94→  beers: Beer | null;\n    95→  tap_pricing: Pick<TapPricing, 'price_12oz'>[];\n    96→};\n    97→\n    98→function computeAvailability(\n    99→  ozRemaining: number,\n   100→  lowThresholdOz: number,\n   101→): TapWithBeer['availability_status'] {\n   102→  if (ozRemaining <= 0) return 'out';\n   103→  if (ozRemaining <= lowThresholdOz) return 'low';\n   104→  return 'available';\n   105→}\n   106→\n   107→function mapTapRow(row: TapRow): TapWithBeer {\n   108→  const pricing = row.tap_pricing?.[0];\n   109→  return {\n   110→    id: row.id,\n   111→    venue_id: row.venue_id,\n   112→    tap_number: row.tap_number,\n   113→    beer_id: row.beer_id,\n   114→    status: row.status,\n   115→    oz_remaining: row.oz_remaining,\n   116→    low_threshold_oz: row.low_threshold_oz,\n   117→    temperature_f: row.temperature_f,\n   118→    temp_ok: row.temp_ok,\n   119→    temp_threshold_f: row.temp_threshold_f,\n   120→    created_at: row.created_at,\n   121→    updated_at: row.updated_at,\n   122→    beer: row.beers ?? null,\n   123→    price_12oz: pricing?.price_12oz ?? null,\n   124→    availability_status: computeAvailability(row.oz_remaining, row.low_threshold_oz),\n   125→  };\n   126→}\n   127→\n   128→/**\n   129→ * Fetch active taps for a venue with joined beer name, style, pricing, and\n   130→ * computed availability_status + temp_ok.\n   131→ */\n   132→export async function fetchVenueTaps(venueId: string): Promise<TapWithBeer[]> {\n   133→  const { data, error } = await supabase\n   134→    .from('taps')\n   135→    .select(\n   136→      `\n   137→      *,\n   138→      beers (*),\n   139→      tap_pricing (price_12oz)\n   140→    `,\n   141→    )\n   142→    .eq('venue_id', venueId)\n   143→    .eq('status', 'active')\n   144→    .order('tap_number');\n   145→\n   146→  if (error) throw error;\n   147→  return (data ?? []).map((row: TapRow) => mapTapRow(row));\n   148→}\n   149→\n   150→// ============================================================\n   151→// Realtime\n   152→// ============================================================\n   153→\n   154→/**\n   155→ * Subscribe to realtime changes on the taps table for a specific venue.\n   156→ * Calls `onUpdate` with the changed tap row whenever an UPDATE occurs.\n   157→ * Returns the RealtimeChannel (call `.unsubscribe()` to clean up).\n   158→ */\n   159→export function subscribeTaps(\n   160→  venueId: string,\n   161→  onUpdate: (tap: Tap) => void,\n   162→): RealtimeChannel {\n   163→  return supabase\n   164→    .channel(`taps:venue_id=eq.${venueId}`)\n   165→    .on<Tap>(\n   166→      'postgres_changes',\n   167→      {\n   168→        event: 'UPDATE',\n   169→        schema: 'public',\n   170→        table: 'taps',\n   171→        filter: `venue_id=eq.${venueId}`,\n   172→      },\n   173→      (payload) => {\n   174→        onUpdate(payload.new);\n   175→      },\n   176→    )\n   177→    .subscribe();\n   178→}\n   179→"}]},"parent_tool_use_id":null,"session_id":"98cd6fab-3f7c-4341-b66d-042c8e2478b0","uuid":"c5efb236-71ac-492f-b075-4b1b7836d738","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\check\\beerbot\\beerbot-app\\lib\\api\\venues.ts","content":"import { supabase } from '@/lib/supabase';\nimport type {\n  Beer,\n  Tap,\n  TapPricing,\n  TapWithBeer,\n  Venue,\n  VenueWithDistance,\n} from '@/types/api';\nimport type { RealtimeChannel } from '@supabase/supabase-js';\n\n// ============================================================\n// Venues\n// ============================================================\n\n/**\n * Fetch all active venues, optionally sorted by proximity to the given coordinates.\n * When lat/lng are provided, calls the `get_venues_nearby` RPC function which\n * returns results sorted by distance (Haversine) and includes `distance_miles`.\n * Without coordinates, fetches directly from the venues table sorted by name.\n */\nexport async function fetchVenues(params?: {\n  latitude?: number;\n  longitude?: number;\n}): Promise<VenueWithDistance[]> {\n  if (params?.latitude != null && params?.longitude != null) {\n    const { data, error } = await supabase.rpc('get_venues_nearby', {\n      user_lat: params.latitude,\n      user_lng: params.longitude,\n    });\n\n    if (error) throw error;\n    return (data ?? []) as VenueWithDistance[];\n  }\n\n  // No coordinates — simple fetch sorted by name\n  const { data, error } = await supabase\n    .from('venues')\n    .select('*')\n    .eq('is_active', true)\n    .order('name');\n\n  if (error) throw error;\n\n  return (data ?? []).map((v: Venue) => ({\n    ...v,\n    distance_miles: null,\n  }));\n}\n\n/**\n * Fetch a single venue by ID.\n */\nexport async function fetchVenue(venueId: string): Promise<Venue> {\n  const { data, error } = await supabase\n    .from('venues')\n    .select('*')\n    .eq('id', venueId)\n    .single();\n\n  if (error) throw error;\n  return data as Venue;\n}\n\n/**\n * Fetch active tap counts for a list of venue IDs.\n * Returns a map of venueId -> active tap count.\n */\nexport async function fetchVenueActiveTapCounts(\n  venueIds: string[],\n): Promise<Record<string, number>> {\n  if (venueIds.length === 0) return {};\n\n  const { data, error } = await supabase\n    .from('taps')\n    .select('venue_id')\n    .in('venue_id', venueIds)\n    .eq('status', 'active');\n\n  if (error) throw error;\n\n  const counts: Record<string, number> = {};\n  for (const row of data ?? []) {\n    counts[row.venue_id] = (counts[row.venue_id] ?? 0) + 1;\n  }\n  return counts;\n}\n\n// ============================================================\n// Taps\n// ============================================================\n\ntype TapRow = Tap & {\n  beers: Beer | null;\n  tap_pricing: Pick<TapPricing, 'price_12oz'>[];\n};\n\nfunction computeAvailability(\n  ozRemaining: number,\n  lowThresholdOz: number,\n): TapWithBeer['availability_status'] {\n  if (ozRemaining <= 0) return 'out';\n  if (ozRemaining <= lowThresholdOz) return 'low';\n  return 'available';\n}\n\nfunction mapTapRow(row: TapRow): TapWithBeer {\n  const pricing = row.tap_pricing?.[0];\n  return {\n    id: row.id,\n    venue_id: row.venue_id,\n    tap_number: row.tap_number,\n    beer_id: row.beer_id,\n    status: row.status,\n    oz_remaining: row.oz_remaining,\n    low_threshold_oz: row.low_threshold_oz,\n    temperature_f: row.temperature_f,\n    temp_ok: row.temp_ok,\n    temp_threshold_f: row.temp_threshold_f,\n    created_at: row.created_at,\n    updated_at: row.updated_at,\n    beer: row.beers ?? null,\n    price_12oz: pricing?.price_12oz ?? null,\n    availability_status: computeAvailability(row.oz_remaining, row.low_threshold_oz),\n  };\n}\n\n/**\n * Fetch active taps for a venue with joined beer name, style, pricing, and\n * computed availability_status + temp_ok.\n */\nexport async function fetchVenueTaps(venueId: string): Promise<TapWithBeer[]> {\n  const { data, error } = await supabase\n    .from('taps')\n    .select(\n      `\n      *,\n      beers (*),\n      tap_pricing (price_12oz)\n    `,\n    )\n    .eq('venue_id', venueId)\n    .eq('status', 'active')\n    .order('tap_number');\n\n  if (error) throw error;\n  return (data ?? []).map((row: TapRow) => mapTapRow(row));\n}\n\n// ============================================================\n// Realtime\n// ============================================================\n\n/**\n * Subscribe to realtime changes on the taps table for a specific venue.\n * Calls `onUpdate` with the changed tap row whenever an UPDATE occurs.\n * Returns the RealtimeChannel (call `.unsubscribe()` to clean up).\n */\nexport function subscribeTaps(\n  venueId: string,\n  onUpdate: (tap: Tap) => void,\n): RealtimeChannel {\n  return supabase\n    .channel(`taps:venue_id=eq.${venueId}`)\n    .on<Tap>(\n      'postgres_changes',\n      {\n        event: 'UPDATE',\n        schema: 'public',\n        table: 'taps',\n        filter: `venue_id=eq.${venueId}`,\n      },\n      (payload) => {\n        onUpdate(payload.new);\n      },\n    )\n    .subscribe();\n}\n","numLines":179,"startLine":1,"totalLines":179}}}
