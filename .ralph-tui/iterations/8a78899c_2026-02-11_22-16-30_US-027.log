─────────────────────────────────\n    49→\n    50→export default function VerifyAgeScreen() {\n    51→  const { tapId, venueId, quantity, totalPrice } = useLocalSearchParams<{\n    52→    tapId: string;\n    53→    venueId: string;\n    54→    quantity: string;\n    55→    totalPrice: string;\n    56→  }>();\n    57→  const router = useRouter();\n    58→  const insets = useSafeAreaInsets();\n    59→\n    60→  const [screenState, setScreenState] = useState<ScreenState>('checking');\n    61→  const [sessionUrl, setSessionUrl] = useState<string | null>(null);\n    62→  const [rememberVerification, setRememberVerification] = useState(true);\n    63→  const [attempts, setAttempts] = useState(0);\n    64→  const [failureReason, setFailureReason] = useState('');\n    65→  const webviewRef = useRef<WebView>(null);\n    66→  const hasNavigated = useRef(false);\n    67→\n    68→  // Forward params for the payment screen (memoized to avoid dependency churn)\n    69→  const orderParams = useMemo(\n    70→    () => ({\n    71→      tapId: tapId ?? '',\n    72→      venueId: venueId ?? '',\n    73→      quantity: quantity ?? '1',\n    74→      totalPrice: totalPrice ?? '0',\n    75→    }),\n    76→    [tapId, venueId, quantity, totalPrice],\n    77→  );\n    78→\n    79→  // ─────────────────────────────────────────────────\n    80→  // Check existing verification status\n    81→  // ─────────────────────────────────────────────────\n    82→\n    83→  const statusQuery = useQuery({\n    84→    queryKey: ['verification-status'],\n    85→    queryFn: checkVerificationStatus,\n    86→    staleTime: 1000 * 10,\n    87→    retry: false,\n    88→  });\n    89→\n    90→  // Auto-skip if already verified\n    91→  useEffect(() => {\n    92→    if (statusQuery.isLoading) return;\n    93→    if (hasNavigated.current) return;\n    94→\n    95→    if (statusQuery.data?.age_verified) {\n    96→      hasNavigated.current = true;\n    97→      router.replace({\n    98→        pathname: '/(main)/order/payment',\n    99→        params: orderParams,\n   100→      });\n   101→    } else if (statusQuery.isSuccess) {\n   102→      setScreenState('explanation');\n   103→    } else if (statusQuery.isError) {\n   104→      // Even on error, show explanation — verification will be attempted anyway\n   105→      setScreenState('explanation');\n   106→    }\n   107→  }, [\n   108→    statusQuery.isLoading,\n   109→    statusQuery.data,\n   110→    statusQuery.isSuccess,\n   111→    statusQuery.isError,\n   112→    router,\n   113→    orderParams,\n   114→  ]);\n   115→\n   116→  // ─────────────────────────────────────────────────\n   117→  // Start Verification Session\n   118→  // ─────────────────────────────────────────────────\n   119→\n   120→  const startVerification = useCallback(async () => {\n   121→    if (attempts >= MAX_ATTEMPTS) {\n   122→      Alert.alert(\n   123→        'Too Many Attempts',\n   124→        'You have exceeded the maximum number of verification attempts. Please contact support.',\n   125→        [{ text: 'OK', onPress: () => router.back() }],\n   126→      );\n   127→      return;\n   128→    }\n   129→\n   130→    setScreenState('loading_session');\n   131→    setFailureReason('');\n   132→\n   133→    try {\n   134→      const session = await createVerificationSession();\n   135→      setSessionUrl(session.session_url);\n   136→      setScreenState('verifying');\n   137→      setAttempts((prev) => prev + 1);\n   138→    } catch (err) {\n   139→      const message =\n   140→        err instanceof Error ? err.message : 'Failed to start verification';\n   141→\n   142→      if (message.includes('rate') || message.includes('429')) {\n   143→        Alert.alert(\n   144→          'Rate Limited',\n   145→          'Too many verification attempts. Please try again later.',\n   146→          [{ text: 'OK' }],\n   147→        );\n   148→        setScreenState('explanation');\n   149→      } else if (message.includes('already verified') || message.includes('400')) {\n   150→        // Race condition: user got verified between check and start\n   151→        hasNavigated.current = true;\n   152→        router.replace({\n   153→          pathname: '/(main)/order/payment',\n   154→          params: orderParams,\n   155→        });\n   156→      } else {\n   157→        Alert.alert('Error', message, [{ text: 'OK' }]);\n   158→        setScreenState('explanation');\n   159→      }\n   160→    }\n   161→  }, [attempts, router, orderParams]);\n   162→\n   163→  // ─────────────────────────────────────────────────\n   164→  // Handle Veriff WebView Navigation\n   165→  // ─────────────────────────────────────────────────\n   166→\n   167→  const handleNavigationChange = useCallback(\n   168→    (navState: WebViewNavigation) => {\n   169→      const { url } = navState;\n   170→      if (!url) return;\n   171→\n   172→      // Veriff redirects to a result URL on completion\n   173→      if (url.includes('/result') || url.includes('status=')) {\n   174→        if (url.includes('approved') || url.includes('success') || url.includes('9001')) {\n   175→          setScreenState('processing');\n   176→          // Poll for verification status update\n   177→          const pollInterval = setInterval(async () => {\n   178→            try {\n   179→              const status = await checkVerificationStatus();\n   180→              if (status.age_verified) {\n   181→                clearInterval(pollInterval);\n   182→                setScreenState('success');\n   183→                setTimeout(() => {\n   184→                  if (!hasNavigated.current) {\n   185→                    hasNavigated.current = true;\n   186→                    router.replace({\n   187→                      pathname: '/(main)/order/payment',\n   188→                      params: orderParams,\n   189→                    });\n   190→                  }\n   191→                }, 1200);\n   192→              }\n   193→            } catch {\n   194→              // Keep polling\n   195→            }\n   196→          }, 2000);\n   197→\n   198→          // Stop polling after 30s\n   199→          setTimeout(() => {\n   200→            clearInterval(pollInterval);\n   201→            if (screenState === 'processing') {\n   202→              // Webhook might be slow — navigate anyway, payment screen will re-check\n   203→              hasNavigated.current = true;\n   204→              router.replace({\n   205→                pathname: '/(main)/order/payment',\n   206→                params: orderParams,\n   207→              });\n   208→            }\n   209→          }, 30000);\n   210→        } else if (url.includes('declined') || url.includes('failed') || url.includes('9102')) {\n   211→          setFailureReason(mapFailureReason(url));\n   212→          setScreenState('failed');\n   213→        } else if (url.includes('resubmit') || url.includes('9103')) {\n   214→          setFailureReason('ID not readable. Please ensure your ID is clear and well-lit.');\n   215→          setScreenState('failed');\n   216→        } else if (url.includes('expired') || url.includes('9104')) {\n   217→          setFailureReason('Session expired. Please try again.');\n   218→          setScreenState('failed');\n   219→        }\n   220→      }\n   221→    },\n   222→    [router, orderParams, screenState],\n   223→  );\n   224→\n   225→  // ─────────────────────────────────────────────────\n   226→  // Handle WebView errors\n   227→  // ─────────────────────────────────────────────────\n   228→\n   229→  const handleWebViewError = useCallback(() => {\n   230→    setFailureReason('A connection error occurred. Please check your network and try again.');\n   231→    setScreenState('failed');\n   232→  }, []);\n   233→\n   234→  // ─────────────────────────────────────────────────\n   235→  // Render: Checking State\n   236→  // ─────────────────────────────────────────────────\n   237→\n   238→  if (screenState === 'checking') {\n   239→    return (\n   240→      <View\n   241→        className=\"flex-1 bg-dark items-center justify-center\"\n   242→        style={{ paddingTop: insets.top }}\n   243→      >\n   244→        <ActivityIndicator color=\"#f59e0b\" size=\"large\" />\n   245→        <Text className=\"text-white/40 text-sm mt-4\">\n   246→          Checking verification status...\n   247→        </Text>\n   248→      </View>\n   249→    );\n   250→  }\n   251→\n   252→  // ─────────────────────────────────────────────────\n   253→  // Render: Veriff WebView\n   254→  // ─────────────────────────────────────────────────\n   255→\n   256→  if (screenState === 'verifying' && sessionUrl) {\n   257→    return (\n   258→      <View className=\"flex-1 bg-dark\" style={{ paddingTop: insets.top }}>\n   259→        {/* Header with cancel */}\n   260→        <View className=\"px-6 py-3 flex-row items-center justify-between border-b border-dark-600\">\n   261→          <Text className=\"text-lg font-semibold text-white\">\n   262→            Age Verification\n   263→          </Text>\n   264→          <Pressable\n   265→            onPress={() => {\n   266→              Alert.alert(\n   267→                'Cancel Verification?',\n   268→                'Your progress will be lost.',\n   269→                [\n   270→                  { text: 'Continue Verifying', style: 'cancel' },\n   271→                  {\n   272→                    text: 'Cancel',\n   273→                    style: 'destructive',\n   274→                    onPress: () => setScreenState('explanation'),\n   275→                  },\n   276→                ],\n   277→              );\n   278→            }}\n   279→            hitSlop={16}\n   280→            className=\"active:opacity-60\"\n   281→          >\n   282→            <Text className=\"text-brand text-base\">Cancel</Text>\n   283→          </Pressable>\n   284→        </View>\n   285→\n   286→        <WebView\n   287→          ref={webviewRef}\n   288→          s