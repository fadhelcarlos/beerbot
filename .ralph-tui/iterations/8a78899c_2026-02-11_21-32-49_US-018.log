onst loc = await Location.getCurrentPositionAsync({\n   219→          accuracy: Location.Accuracy.Balanced,\n   220→        });\n   221→        if (cancelled) return;\n   222→\n   223→        // Haversine approximation for short distances\n   224→        const lat1 = loc.coords.latitude;\n   225→        const lon1 = loc.coords.longitude;\n   226→        const lat2 = Number(venueQuery.data.latitude);\n   227→        const lon2 = Number(venueQuery.data.longitude);\n   228→        const R = 3959; // Earth radius in miles\n   229→        const dLat = ((lat2 - lat1) * Math.PI) / 180;\n   230→        const dLon = ((lon2 - lon1) * Math.PI) / 180;\n   231→        const a =\n   232→          Math.sin(dLat / 2) * Math.sin(dLat / 2) +\n   233→          Math.cos((lat1 * Math.PI) / 180) *\n   234→            Math.cos((lat2 * Math.PI) / 180) *\n   235→            Math.sin(dLon / 2) *\n   236→            Math.sin(dLon / 2);\n   237→        const c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));\n   238→        const distanceMiles = R * c;\n   239→\n   240→        setIsFarFromVenue(distanceMiles > FAR_THRESHOLD_MILES);\n   241→      } catch {\n   242→        // Silently ignore location errors\n   243→      }\n   244→    })();\n   245→\n   246→    return () => {\n   247→      cancelled = true;\n   248→    };\n   249→  }, [venueQuery.data]);\n   250→\n   251→  // Subscribe to realtime tap updates\n   252→  useEffect(() => {\n   253→    if (!id) return;\n   254→\n   255→    channelRef.current = subscribeTaps(id, (updatedTap: Tap) => {\n   256→      queryClient.setQueryData<TapWithBeer[]>(\n   257→        ['venue-taps', id],\n   258→        (prev) => {\n   259→          if (!prev) return prev;\n   260→          return prev.map((tap) => {\n   261→            if (tap.id !== updatedTap.id) return tap;\n   262→            // Merge updated tap fields, keep beer and pricing from cache\n   263→            const ozRemaining = updatedTap.oz_remaining;\n   264→            const lowThreshold = 