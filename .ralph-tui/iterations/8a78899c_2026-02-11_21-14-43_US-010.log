t supabase\n    58→    .from('orders')\n    59→    .select('*')\n    60→    .order('created_at', { ascending: false })\n    61→    .range(offset, offset + limit - 1);\n    62→\n    63→  if (error) throw error;\n    64→  return (data ?? []) as Order[];\n    65→}\n    66→"}]},"parent_tool_use_id":null,"session_id":"105bc883-5cda-4f2f-9ba4-aff9cfa15c01","uuid":"ed9049e1-bfb0-4bf1-8fa6-774e0ed735d4","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\check\\beerbot\\beerbot-app\\lib\\api\\orders.ts","content":"import { supabase } from '@/lib/supabase';\nimport type {\n  CreateOrderRequest,\n  CreateOrderResponse,\n  Order,\n} from '@/types/api';\n\n/**\n * Create a new order by calling the create-order Edge Function.\n * The Edge Function validates age verification, tap/venue status,\n * inventory, and temperature, then atomically creates the order\n * and decrements inventory via a database RPC.\n */\nexport async function createOrder(\n  request: CreateOrderRequest,\n): Promise<CreateOrderResponse> {\n  const { data, error } = await supabase.functions.invoke<CreateOrderResponse>(\n    'create-order',\n    {\n      method: 'POST',\n      body: request,\n    },\n  );\n\n  if (error) throw error;\n  if (!data) throw new Error('No data returned from create-order');\n\n  return data;\n}\n\n/**\n * Fetch a single order by ID. RLS ensures only the owning user\n * can read their own orders.\n */\nexport async function getOrder(orderId: string): Promise<Order> {\n  const { data, error } = await supabase\n    .from('orders')\n    .select('*')\n    .eq('id', orderId)\n    .single();\n\n  if (error) throw error;\n  return data as Order;\n}\n\n/**\n * Fetch the current user's order history with pagination.\n * Returns newest orders first.\n */\nexport async function getOrderHistory(params?: {\n  limit?: number;\n  offset?: number;\n}): Promise<Order[]> {\n  const limit = params?.limit ?? 20;\n  const offset = params?.offset ?? 0;\n\n  const { data, error } = await supabase\n    .from('orders')\n    .select('*')\n    .order('created_at', { ascending: false })\n    .range(offset, offset + limit - 1);\n\n  if (error) throw error;\n  return (data ?? []) as Order[];\n}\n","numLines":66,"startLine":1,"totalLines":66}}}
