f (orderRow.status === \"cancelled\" || orderRow.status === \"refunded\") {\n   204→    console.log(`Order ${orderId} already in status ${orderRow.status}, skipping`);\n   205→    return;\n   206→  }\n   207→\n   208→  // Update order status to 'cancelled'\n   209→  await supabaseAdmin\n   210→    .from(\"orders\")\n   211→    .update({ status: \"cancelled\" })\n   212→    .eq(\"id\", orderId);\n   213→\n   214→  // Restore oz_remaining on the tap (read-then-write since Supabase JS\n   215→  // doesn't support SET oz_remaining = oz_remaining + X directly)\n   216→  const restoreOz = orderRow.quantity * orderRow.pour_size_oz;\n   217→  const { data: tap } = await supabaseAdmin\n   218→    .from(\"taps\")\n   219→    .select(\"oz_remaining\")\n   220→    .eq(\"id\", orderRow.tap_id)\n   221→    .single();\n   222→\n   223→  if (tap) {\n   224→    const newOz = Number(tap.oz_remaining) + restoreOz;\n   225→    await supabaseAdmin\n   226→      .from(\"taps\")\n   227→      .update({ oz_remaining: newOz })\n   228→      .eq(\"id\", orderRow.tap_id);\n   229→  }\n   230→\n   231→  // Log payment failed event\n   232→  await supabaseAdmin.from(\"order_events\").insert({\n   233→    order_id: orderId,\n   234→    event_type: \"stripe_payment_intent.payment_failed\",\n   235→    metadata: {\n   236→      stripe_event_id: event.id,\n   237→      payment_intent_id: paymentIntent.id,\n   238→      failure_code: paymentIntent.last_payment_error?.code ?? null,\n   239→      failure_message: paymentIntent.last_payment_error?.message ?? null,\n   240→    },\n   241→  });\n   242→}\n   243→\n   244→async function handleChargeRefunded(\n   245→  event: Stripe.Event,\n   246→  supabaseAdmin: ReturnType<typeof createClient>,\n   247→): Promise<void> {\n   248→  const charge = event.data.object as Stripe.Charge;\n   249→  const paymentIntentId = charge.payment_intent as string | null;\n   250→\n   251→  if (!paymentIntentId) {\n   252→    console.error(\"No payment_intent on refunded c