      )}\n   234→            </View>\n   235→\n   236→            {/* Password */}\n   237→            <View className=\"mt-5\">\n   238→              <Text className=\"text-sm text-white/70 mb-2\">Password</Text>\n   239→              <View className=\"relative\">\n   240→                <TextInput\n   241→                  className=\"bg-dark-700 rounded-xl px-4 py-3.5 pr-16 text-white text-base\"\n   242→                  placeholder=\"Min 8 characters\"\n   243→                  placeholderTextColor=\"rgba(255,255,255,0.25)\"\n   244→                  value={password}\n   245→                  onChangeText={setPassword}\n   246→                  onBlur={() => setTouched((t) => ({ ...t, password: true }))}\n   247→                  secureTextEntry={!showPassword}\n   248→                  autoCapitalize=\"none\"\n   249→                  autoComplete=\"new-password\"\n   250→                  autoCorrect={false}\n   251→                  returnKeyType=\"done\"\n   252→                  editable={!isLoading}\n   253→                  onSubmitEditing={handleRegister}\n   254→                />\n   255→                <Pressable\n   256→                  onPress={() => setShowPassword((v) => !v)}\n   257→                  className=\"absolute right-4 top-0 bottom-0 justify-center active:opacity-60\"\n   258→                  hitSlop={8}\n   259→                >\n   260→                  <Text className=\"text-brand text-sm font-medium\">\n   261→                    {showPassword ? 'Hide' : 'Show'}\n   262→                  </Text>\n   263→                </Pressable>\n   264→              </View>\n   265→              {(passwordError || passwordEmpty) && (\n   266→                <Text className=\"text-red-400 text-xs mt-1\">{passwordError ?? passwordEmpty}</Text>\n   267→              )}\n   268→              <PasswordStrengthBar password={password} />\n   269→            </View>\n   270→          </View>\n   271→\n   272→          {/* Submit button */}\n   273→  