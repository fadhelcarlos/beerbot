───────────────────────────────────────────\n    50→\n    51→export default function VerifyAgeScreen() {\n    52→  const { tapId, venueId, quantity, totalPrice } = useLocalSearchParams<{\n    53→    tapId: string;\n    54→    venueId: string;\n    55→    quantity: string;\n    56→    totalPrice: string;\n    57→  }>();\n    58→  const router = useRouter();\n    59→  const insets = useSafeAreaInsets();\n    60→\n    61→  const [screenState, setScreenState] = useState<ScreenState>('checking');\n    62→  const [sessionUrl, setSessionUrl] = useState<string | null>(null);\n    63→  const [rememberVerification, setRememberVerification] = useState(true);\n    64→  const [attempts, setAttempts] = useState(0);\n    65→  const [failureReason, setFailureReason] = useState('');\n    66→  const webviewRef = useRef<WebView>(null);\n    67→  const hasNavigated = useRef(false);\n    68→\n    69→  // Forward params for the payment screen\n    70→  const orderParams = {\n    71→    tapId: tapId ?? '',\n    72→    venueId: venueId ?? '',\n    73→    quantity: quantity ?? '1',\n    74→    totalPrice: totalPrice ?? '0',\n    75→  };\n    76→\n    77→  // ─────────────────────────────────────────────────\n    78→  // Check existing verification status\n    79→  // ─────────────────────────────────────────────────\n    80→\n    81→  const statusQuery = useQuery({\n    82→    queryKey: ['verification-status'],\n    83→    queryFn: checkVerificationStatus,\n    84→    staleTime: 1000 * 10,\n    85→    retry: false,\n    86→  });\n    87→\n    88→  // Auto-skip if already verified\n    89→  useEffect(() => {\n    90→    if (statusQuery.isLoading) return;\n    91→    if (hasNavigated.current) return;\n    92→\n    93→    if (statusQuery.data?.age_verified) {\n    94→      hasNavigated.current = true;\n    95→      router.replace({\n    96→        pathname: '/(main)/order/payment',\n    97→        params: orderParams,\n    98→      });\n    99→    } else if (statusQuery.isSuccess) {\n   100→      setScreenState('explanation');\n   101→    } else if (statusQuery.isError) {\n   102→      // Even on error, show explanation — verification will be attempted anyway\n   103→      setScreenState('explanation');\n   104→    }\n   105→  }, [\n   106→    statusQuery.isLoading,\n   107→    statusQuery.data,\n   108→    statusQuery.isSuccess,\n   109→    statusQuery.isError,\n   110→    router,\n   111→    orderParams.tapId,\n   112→    orderParams.venueId,\n   113→  ]);\n   114→\n   115→  // ─────────────────────────────────────────────────\n   116→  // Start Verification Session\n   117→  // ─────────────────────────────────────────────────\n   118→\n   119→  const startVerification = useCallback(async () => {\n   120→    if (attempts >= MAX_ATTEMPTS) {\n   121→      Alert.alert(\n   122→        'Too Many Attempts',\n   123→        'You have exceeded the maximum number of verification attempts. Please contact support.',\n   124→        [{ text: 'OK', onPress: () => router.back() }],\n   125→      );\n   126→      return;\n   127→    }\n   128→\n   129→    setScreenState('loading_session');\n   130→    setFailureReason('');\n   131→\n   132→    try {\n   133→      const session = await createVerificationSession();\n   134→      setSessionUrl(session.session_url);\n   135→      setScreenState('verifying');\n   136→      setAttempts((prev) => prev + 1);\n   137→    } catch (err) {\n   138→      const message =\n   139→        err instanceof Error ? err.message : 'Failed to start verification';\n   140→\n   141→      if (message.includes('rate') || message.includes('429')) {\n   142→        Alert.alert(\n   143→          'Rate Limited',\n   144→          'Too many verification attempts. Please try again later.',\n   145→          [{ text: 'OK' }],\n   146→        );\n   147→        setScreenState('explanation');\n   148→      } else if (message.includes('already verified') || message.includes('400')) {\n   149→        // Race condition: user got verified between check and start\n   150→        hasNavigated.current = true;\n   151→        router.replace({\n   152→          pathname: '/(main)/order/payment',\n   153→          params: orderParams,\n   154→        });\n   155→      } else {\n   156→        Alert.alert('Error', message, [{ text: 'OK' }]);\n   157→        setScreenState('explanation');\n   158→      }\n   159→    }\n   160→  }, [attempts, router, orderParams]);\n   161→\n   162→  // ─────────────────────────────────────────────────\n   163→  // Handle Veriff WebView Navigation\n   164→  // ─────────────────────────────────────────────────\n   165→\n   166→  const handleNavigationChange = useCallback(\n   167→    (navState: WebViewNavigation) => {\n   168→      const { url } = navState;\n   169→      if (!url) return;\n   170→\n   171→      // Veriff redirects to a result URL on completion\n   172→      if (url.includes('/result') || url.includes('status=')) {\n   173→        if (url.includes('approved') || url.includes('success') || url.includes('9001')) {\n   174→          setScreenState('processing');\n   175→          // Poll for verification status update\n   176→          const pollInterval = setInterval(async () => {\n   177→            try {\n   178→              const status = await checkVerificationStatus();\n   179→              if (status.age_verified) {\n   180→                clearInterval(pollInterval);\n   181→                setScreenState('success');\n   182→                setTimeout(() => {\n   183→                  if (!hasNavigated.current) {\n   184→                    hasNavigated.current = true;\n   185→                    router.replace({\n   186→                      pathname: '/(main)/order/payment',\n   187→                      params: orderParams,\n   188→                    });\n   189→                  }\n   190→                }, 1200);\n   191→              }\n   192→            } catch {\n   193→              // Keep polling\n   194→            }\n   195→          }, 2000);\n   196→\n   197→          // Stop polling after 30s\n   198→          setTimeout(() => {\n   199→            clearInterval(pollInterval);\n   200→            if (screenState === 'processing') {\n   201→              // Webhook might be slow — navigate anyway, payment screen will re-check\n   202→              hasNavigated.current = true;\n   203→              router.replace({\n   204→                pathname: '/(main)/order/payment',\n   205→                params: orderParams,\n   206→              });\n   207→            }\n   208→          }, 30000);\n   209→        } else if (url.includes('declined') || url.includes('failed') || url.includes('9102')) {\n   210→          setFailureReason(mapFailureReason(url));\n   211→          setScreenState('failed');\n   212→        } else if (url.includes('resubmit') || url.includes('9103')) {\n   213→          setFailureReason('ID not readable. Please ensure your ID is clear and well-lit.');\n   214→          setScreenState('failed');\n   215→        } else if (url.includes('expired') || url.includes('9104')) {\n   216→          setFailureReason('Session expired. Please try again.');\n   217→          setScreenState('failed');\n   218→        }\n   219→      }\n   220→    },\n   221→    [router, orderParams, screenState],\n   222→  );\n   223→\n   224→  // ─────────────────────────────────────────────────\n   225→  // Handle WebView errors\n   226→  // ─────────────────────────────────────────────────\n   227→\n   228→  const handleWebViewError = useCallback(() => {\n   229→    setFailureReason('A connection error occurred. Please check your network and try again.');\n   230→    setScreenState('failed');\n   231→  }, []);\n   232→\n   233→  // ─────────────────────────────────────────────────\n   234→  // Handle camera permission denied in WebView\n   235→  // ─────────────────────────────────────────────────\n   236→\n   237→  const handleCameraPermissionDenied = useCallback(() => {\n   238→    Alert.alert(\n   239→      'Camera Access Required',\n   240→      'BeerBot needs camera access for age verification. Please enable it in your device settings.',\n   241→      [\n   242→        { text: 'Cancel', style: 'cancel' },\n   243→        {\n   244→          text: 'Open Settings',\n   245→          onPress: () => Linking.openSettings(),\n   246→        },\n   247→      ],\n   248→    );\n   249→    setScreenState('explanation');\n   250→  }, []);\n   251→\n   252→  // ─────────────────────────────────────────────────\n   253→  // Render: Checking State\n   254→  // ─────────────────────────────────────────────────\n   255→\n   256→  if (screenState === 'checking') {\n   257→    return (\n   258→      <View\n   259→        className=\"flex-1 bg-dark items-center justify-center\"\n   260→        style={{ paddingTop: insets.top }}\n   261→      >\n   262→        <ActivityIndicator color=\"#f59e0b\" size=\"large\" />\n   263→        <Text className=\"text-white/40 text-sm mt-4\">\n   264→          Checking verification status...\n   265→        </Text>\n   266→      </View>\n   267→    );\n   268→  }\n   269→\n   270→  // ─────────────────────────────────────────────────\n   271→  // Render: Veriff WebView\n   272→  // ─────────────────────────────────────────────────\n   273→\n   274→  if (screenState === 'verifying' && sessionUrl) {\n   275→    return (\n   276→      <View className=\"flex-1 bg-dark\" style={{ paddingTop: insets.top }}>\n   277→        {/* Header with cancel */}\n   278→        <View className=\"px-6 py-3 flex-row items-center justify-between border-b border-dark-600\">\n   279→          <Text className=\"text-lg font-semibold text-white\">\n   280→            Age Verification\n   281→          </Text>\n   282→          <Pressable\n   283→            onPress={() => {\n   284→              Alert.alert(\n   285→                'Cancel Verification?',\n   286→                'Your progress will be lost.',\n   287→                [\n   288→                  { text: 'Continue Verifying', style: 'cancel' },\n   289→                  {\n   290→                    text: 'Cancel',\n   291→                    style: 'destructive',\n   292→                    onPress: () => setScreenState('explanation'),\n   293→                  },\n   294→                ],\n   295→              );\n   296→            }}\n   297→            hitSlop={16}\n   298→            className=\"active:opacity-60\"\n   299→          >\n   300→            <Text className=\"text-brand text-base\">Cancel</Text>\n   301→          </Pressable>\n   302→        </View>\n   303→\n   304→        <WebView\n   305→          ref={webviewRef}\n   306→          source={{ uri: sessionUrl }}\n   307→          style={{ flex: 1 }}\n   308→          javaScriptEnabled\n   309→          domStorageEnabled\n   310→          mediaPlaybackRequiresUserAction={false}\n   311→          allowsInlineMediaPlayback\n   312→          mediaCapturePermissionGrantType=\"grant\"\n   313→          onNavigationStateChange={handleNavigationChange}\n   314→          onError={handleWebViewError}\n   315→          onHttpError={(syntheticEvent) => {\n   316→            const { statusCode } = syntheticEvent.nativeEvent;\n   317→            if (statusCode >= 400) {\n   318→              handleWebViewError();\n   319→            }\n   320→          }}\n   321→          startInLoadingState\n   322→          renderLoading={() => (\n   323→            <View className=\"absolute inset-0 bg-dark items-center justify-center\">\n   324→              <ActivityIndicator color=\"#f59e0b\" size=\"large\" />\n   325→              <Text className=\"text-white/40 text-sm mt-4\">\n   326→                Loading verification...\n   327→              </Text>\n   328→            </View>\n   329→          )}\n   330→          {...(Platform.OS === 'android' && {\n   331→            androidLayerType: 'hardware',\n   332→            allowFileAccess: true,\n   333→          })}\n   334→        />\n   335→      </View>\n   336→    );\n   337→  }\n   338→\n   339→  // ─────────────────────────────────────────────────\n   340→  // Render: Processing State\n   341→  // ─────────────────────────────────────────────────\n   342→\n   343→  if (screenState === 'processing' || screenState === 'success') {\n   344→    return (\n   345→      <View\n   346→        className=\"flex-1 bg-dark items-center justify-center px-8\"\n   347→        style={{ paddingTop: insets.top }}\n   348→      >\n   349→        {screenState === 'processing' ? (\n   350→          <>\n   351→            <ActivityIndicator color=\"#f59e0b\" size=\"large\" />\n   352→            <Text className=\"text-white text-lg font-semibold mt-6 text-center\">\n   353→              Verifying your identity...\n   354→            </Text>\n   355→            <Text className=\"text-white/40 text-sm mt-2 text-center\">\n   356→              This may take a moment\n   357→            </Text>\n   358→          </>\n   359→        ) : (\n   360→          <Animated.View entering={FadeIn.duration(400)} className=\"items-center\">\n   361→            <Text className=\"text-5xl\">{'\\u2705'}</Text>\n   362→            <Text className=\"text-white text-lg font-semibold mt-4 text-center\">\n   363→              Age Verified!\n   364→            </Text>\n   365→            <Text className=\"text-white/40 text-sm mt-2 text-center\">\n   366→              Proceeding to payment...\n   367→            </Text>\n   368→          </Animated.View>\n   369→        )}\n   370→      </View>\n   371→    );\n   372→  }\n   373→\n   374→  // ─────────────────────────────────────────────────\n   375→  // Render: Loading Session\n   376→  // ─────────────────────────────────────────────────\n   377→\n   378→  if (screenState === 'loading_session') {\n   379→    return (\n   380→      <View\n   381→        className=\"flex-1 bg-dark items-center justify-center\"\n   382→        style={{ paddingTop: insets.top }}\n   383→      >\n   384→        <ActivityIndicator color=\"#f59e0b\" size=\"large\" />\n   385→        <Text className=\"text-white/40 text-sm mt-4\">\n   386→          Starting verification session...\n   387→        </Text>\n   388→      </View>\n   389→    );\n   390→  }\n   391→\n   392→  // ─────────────────────────────────────────────────\n   393→  // Render: Failed State\n   394→  // ─────────────────────────────────────────────────\n   395→\n   396→  if (screenState === 'failed') {\n   397→    const canRetry = attempts < MAX_ATTEMPTS;\n   398→\n   399→    return (\n   400→      <View\n   401→        className=\"flex-1 bg-dark\"\n   402→        style={{ paddingTop: insets.top, paddingBottom: insets.bottom + 16 }}\n   403→      >\n   404→        <View className=\"flex-1 items-center justify-center px-8\">\n   405→          <Animated.View entering={FadeIn.duration(400)} className=\"items-center\">\n   406→            <Text className=\"text-5xl\">{'\\u274C'}</Text>\n   407→            <Text className=\"text-white text-xl font-bold mt-6 text-center\">\n   408→              Verification Failed\n   409→            </Text>\n   410→            <Text className=\"text-white/50 text-base mt-3 text-center leading-6\">\n   411→              {failureReason}\n   412→            </Text>\n   413→            {canRetry && (\n   414→              <Text className=\"text-white/30 text-sm mt-4 text-center\">\n   415→                Attempt {attempts} of {MAX_ATTEMPTS}\n   416→              </Text>\n   417→            )}\n   418→          </Animated.View>\n   419→        </View>\n   420→\n   421→        <View className=\"px-6\">\n   422→          {canRetry ? (\n   423→            <Pressable\n   424→              onPress={startVerification}\n   425→              className=\"w-full items-center justify-center rounded-2xl py-4 bg-brand active:opacity-80\"\n   426→            >\n   427→              <Text className=\"text-lg font-bold text-dark\">Try Again</Text>\n   428→            </Pressable>\n   429→          ) : (\n   430→            <Pressable\n   431→              onPress={() => router.back()}\n   432→              className=\"w-full items-center justify-center rounded-2xl py-4 bg-dark-600 active:opacity-80\"\n   433→            >\n   434→              <Text className=\"text-lg font-bold text-white\">Go Back</Text>\n   435→            </Pressable>\n   436→          )}\n   437→        </View>\n   438→      </View>\n   439→    );\n   440→  }\n   441→\n   442→  // ─────────────────────────────────────────────────\n   443→  // Render: Explanation Screen (default)\n   444→  // ─────────────────────────────────────────────────\n   445→\n   446→  return (\n   447→    <View\n   448→      className=\"flex-1 bg-dark\"\n   449→      style={{ paddingTop: insets.top, paddingBottom: insets.bottom + 16 }}\n   450→    >\n   451→      {/* Back button */}\n   452→      <Pressable\n   453→        onPress={() => router.back()}\n   454→        className=\"px-6 pt-4 pb-2 self-start active:opacity-60\"\n   455→        hitSlop={16}\n   456→      >\n   457→        <Text className=\"text-brand text-base\">{'\\u2190'} Back</Text>\n   458→      </Pressable>\n   459→\n   460→      <View className=\"flex-1 px-6 justify-center\">\n   461→        {/* Shield icon */}\n   462→        <Animated.View\n   463→          entering={FadeIn.duration(400)}\n   464→          className=\"items-center mb-8\"\n   465→        >\n   466→          <View className=\"w-24 h-24 rounded-full bg-brand/15 items-center justify-center\">\n   467→            <Text className=\"text-5xl\">{'\\uD83D\\uDEE1\\uFE0F'}</Text>\n   468→          </View>\n   469→        </Animated.View>\n   470→\n   471→        {/* Title */}\n   472→        <Animated.View entering={FadeInDown.delay(100).duration(350)}>\n   473→          <Text className=\"text-2xl font-bold text-white text-center\">\n   474→            Age Verification Required\n   475→          </Text>\n   476→          <Text className=\"text-base text-white/50 mt-3 text-center leading-6\">\n   477→            We need to verify you&apos;re 21+ before you can purchase beer. This is a one-time process.\n   478→          </Text>\n   479→        </Animated.View>\n   480→\n   481→        {/* What to expect */}\n   482→        <Animated.View\n   483→          entering={FadeInDown.delay(200).duration(350)}\n   484→          className=\"mt-8 bg-dark-700 rounded-2xl p-5 border border-dark-600\"\n   485→        >\n   486→          <Text className=\"text-sm font-semibold text-white mb-3\">\n   487→            What to expect:\n   488→          </Text>\n   489→          <View className=\"gap-2.5\">\n   490→            <View className=\"flex-row items-center\">\n   491→              <Text className=\"text-sm mr-3\">{'\\uD83D\\uDCF7'}</Text>\n   492→              <Text className=\"text-sm text-white/60 flex-1\">\n   493→                Take a photo of your government ID\n   494→              </Text>\n   495→            </View>\n   496→            <View className=\"flex-row items-center\">\n   497→              <Text className=\"text-sm mr-3\">{'\\uD83E\\uDD33'}</Text>\n   498→              <Text className=\"text-sm text-white/60 flex-1\">\n   499→                Take a quick selfie for verification\n   500→              </Text>\n   501→            </View>\n   502→            <View className=\"flex-row items-center\">\n   503→              <Text className=\"text-sm mr-3\">{'\\u2705'}</Text>\n   504→              <Text className=\"text-sm text-white/60 flex-1\">\n   505→                Complete a brief liveness check\n   506→              </Text>\n   507→            </View>\n   508→          </View>\n   509→        </Animated.View>\n   510→\n   511→        {/* Privacy notice */}\n   512→        <Animated.View\n   513→          entering={FadeInDown.delay(300).duration(350)}\n   514→          className=\"mt-4 px-2\"\n   515→        >\n   516→          <Text className=\"text-xs text-white/30 text-center leading-5\">\n   517→            Your ID is processed securely by Veriff and not stored by BeerBot\n   518→          </Text>\n   519→        </Animated.View>\n   520→\n   521→        {/* Remember verification checkbox */}\n   522→        <Animated.View\n   523→          entering={FadeInDown.delay(400).duration(350)}\n   524→          className=\"mt-6\"\n   525→        >\n   526→          <Pressable\n   527→            onPress={() => setRememberVerification((prev) => !prev)}\n   528→            className=\"flex-row items-center justify-center gap-3 active:opacity-70\"\n   529→          >\n   530→            <View\n   531→              className={`w-5 h-5 rounded border-2 items-center justify-center ${\n   532→                rememberVerification\n   533→                  ? 'bg-brand border-brand'\n   534→                  : 'border-white/30 bg-transparent'\n   535→              }`}\n   536→            >\n   537→              {rememberVerification && (\n   538→                <Text className=\"text-xs text-dark font-bold\">{'\\u2713'}</Text>\n   539→              )}\n   540→            </View>\n   541→            <Text className=\"text-sm text-white/60\">\n   542→              Remember my verification\n   543→            </Text>\n   544→          </Pressable>\n   545→        </Animated.View>\n   546→      </View>\n   547→\n   548→      {/* CTA Button */}\n   549→      <Animated.View\n   550→        entering={FadeIn.delay(500).duration(400)}\n   551→        className=\"px-6\"\n   552→      >\n   553→        <Pressable\n   554→          onPress={startVerification}\n   555→          className=\"w-full items-center justify-center rounded-2xl py-4 bg-brand active:opacity-80\"\n   556→        >\n   557→          <Text className=\"text-lg font-bold text-dark\">Verify My Age</Text>\n   558→        </Pressable>\n   559→      </Animated.View>\n   560→    </View>\n   561→  );\n   562→}\n   563→"}]},"parent_tool_use_id":null,"session_id":"59a62e5a-5bb9-4626-ac55-8e50d9f8239b","uuid":"a6d2fcbc-9035-43c4-aff4-480aa35a0c00","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\check\\beerbot\\beerbot-app\\app\\(main)\\order\\verify-age.tsx","content":"import { useState, useCallback, useEffect, useRef } from 'react';\nimport {\n  View,\n  Text,\n  Pressable,\n  ActivityIndicator,\n  Alert,\n  Linking,\n  Platform,\n} from 'react-native';\nimport { useLocalSearchParams, useRouter } from 'expo-router';\nimport { useSafeAreaInsets } from 'react-native-safe-area-context';\nimport Animated, { FadeIn, FadeInDown } from 'react-native-reanimated';\nimport { useQuery } from '@tanstack/react-query';\nimport { WebView } from 'react-native-webview';\nimport type { WebViewNavigation } from 'react-native-webview';\nimport {\n  checkVerificationStatus,\n  createVerificationSession,\n} from '@/lib/api/verification';\n\nconst MAX_ATTEMPTS = 3;\n\ntype ScreenState =\n  | 'checking'\n  | 'explanation'\n  | 'loading_session'\n  | 'verifying'\n  | 'processing'\n  | 'success'\n  | 'failed';\n\n// ─────────────────────────────────────────────────\n// Failure Reason Display\n// ─────────────────────────────────────────────────\n\nfunction mapFailureReason(url: string): string {\n  if (url.includes('not_readable') || url.includes('document'))\n    return 'ID not readable. Please ensure your ID is clear and well-lit.';\n  if (url.includes('liveness') || url.includes('face'))\n    return 'Liveness check failed. Please look directly at the camera.';\n  if (url.includes('expired'))\n    return 'Session expired. Please try again.';\n  return 'Verification was not successful. Please try again.';\n}\n\n// ─────────────────────────────────────────────────\n// Main Screen\n// ─────────────────────────────────────────────────\n\nexport default function VerifyAgeScreen() {\n  const { tapId, venueId, quantity, totalPrice } = useLocalSearchParams<{\n    tapId: string;\n    venueId: string;\n    quantity: string;\n    totalPrice: string;\n  }>();\n  const router = useRouter();\n  const insets = useSafeAreaInsets();\n\n  const [screenState, setScreenState] = useState<ScreenState>('checking');\n  const [sessionUrl, setSessionUrl] = useState<string | null>(null);\n  const [rememberVerification, setRememberVerification] = useState(true);\n  const [attempts, setAttempts] = useState(0);\n  const [failureReason, setFailureReason] = useState('');\n  const webviewRef = useRef<WebView>(null);\n  const hasNavigated = useRef(false);\n\n  // Forward params for the payment screen\n  const orderParams = {\n    tapId: tapId ?? '',\n    venueId: venueId ?? '',\n    quantity: quantity ?? '1',\n    totalPrice: totalPrice ?? '0',\n  };\n\n  // ─────────────────────────────────────────────────\n  // Check existing verification status\n  // ─────────────────────────────────────────────────\n\n  const statusQuery = useQuery({\n    queryKey: ['verification-status'],\n    queryFn: checkVerificationStatus,\n    staleTime: 1000 * 10,\n    retry: false,\n  });\n\n  // Auto-skip if already verified\n  useEffect(() => {\n    if (statusQuery.isLoading) return;\n    if (hasNavigated.current) return;\n\n    if (statusQuery.data?.age_verified) {\n      hasNavigated.current = true;\n      router.replace({\n        pathname: '/(main)/order/payment',\n        params: orderParams,\n      });\n    } else if (statusQuery.isSuccess) {\n      setScreenState('explanation');\n    } else if (statusQuery.isError) {\n      // Even on error, show explanation — verification will be attempted anyway\n      setScreenState('explanation');\n    }\n  }, [\n    statusQuery.isLoading,\n    statusQuery.data,\n    statusQuery.isSuccess,\n    statusQuery.isError,\n    router,\n    orderParams.tapId,\n    orderParams.venueId,\n  ]);\n\n  // ─────────────────────────────────────────────────\n  // Start Verification Session\n  // ─────────────────────────────────────────────────\n\n  const startVerification = useCallback(async () => {\n    if (attempts >= MAX_ATTEMPTS) {\n      Alert.alert(\n        'Too Many Attempts',\n        'You have exceeded the maximum number of verification attempts. Please contact support.',\n        [{ text: 'OK', onPress: () => router.back() }],\n      );\n      return;\n    }\n\n    setScreenState('loading_session');\n    setFailureReason('');\n\n    try {\n      const session = await createVerificationSession();\n      setSessionUrl(session.session_url);\n      setScreenState('verifying');\n      setAttempts((prev) => prev + 1);\n    } catch (err) {\n      const message =\n        err instanceof Error ? err.message : 'Failed to start verification';\n\n      if (message.includes('rate') || message.includes('429')) {\n        Alert.alert(\n          'Rate Limited',\n          'Too many verification attempts. Please try again later.',\n          [{ text: 'OK' }],\n        );\n        setScreenState('explanation');\n      } else if (message.includes('already verified') || message.includes('400')) {\n        // Race condition: user got verified between check and start\n        hasNavigated.current = true;\n        router.replace({\n          pathname: '/(main)/order/payment',\n          params: orderParams,\n        });\n      } else {\n        Alert.alert('Error', message, [{ text: 'OK' }]);\n        setScreenState('explanation');\n      }\n    }\n  }, [attempts, router, orderParams]);\n\n  // ─────────────────────────────────────────────────\n  // Handle Veriff WebView Navigation\n  // ─────────────────────────────────────────────────\n\n  const handleNavigationChange = useCallback(\n    (navState: WebViewNavigation) => {\n      const { url } = navState;\n      if (!url) return;\n\n      // Veriff redirects to a result URL on completion\n      if (url.includes('/result') || url.includes('status=')) {\n        if (url.includes('approved') || url.includes('success') || url.includes('9001')) {\n          setScreenState('processing');\n          // Poll for verification status update\n          const pollInterval = setInterval(async () => {\n            try {\n              const status = await checkVerificationStatus();\n              if (status.age_verified) {\n                clearInterval(pollInterval);\n                setScreenState('success');\n                setTimeout(() => {\n                  if (!hasNavigated.current) {\n                    hasNavigated.current = true;\n                    router.replace({\n                      pathname: '/(main)/order/payment',\n                      params: orderParams,\n                    });\n                  }\n                }, 1200);\n              }\n            } catch {\n              // Keep polling\n            }\n          }, 2000);\n\n          // Stop polling after 30s\n          setTimeout(() => {\n            clearInterval(pollInterval);\n            if (screenState === 'processing') {\n              // Webhook might be slow — navigate anyway, payment screen will re-check\n              hasNavigated.current = true;\n              router.replace({\n                pathname: '/(main)/order/payment',\n                params: orderParams,\n              });\n            }\n          }, 30000);\n        } else if (url.includes('declined') || url.includes('failed') || url.includes('9102')) {\n          setFailureReason(mapFailureReason(url));\n          setScreenState('failed');\n        } else if (url.includes('resubmit') || url.includes('9103')) {\n          setFailureReason('ID not readable. Please ensure your ID is clear and well-lit.');\n          setScreenState('failed');\n        } else if (url.includes('expired') || url.includes('9104')) {\n          setFailureReason('Session expired. Please try again.');\n          setScreenState('failed');\n        }\n      }\n    },\n    [router, orderParams, screenState],\n  );\n\n  // ─────────────────────────────────────────────────\n  // Handle WebView errors\n  // ─────────────────────────────────────────────────\n\n  const handleWebViewError = useCallback(() => {\n    setFailureReason('A connection error occurred. Please check your network and try again.');\n    setScreenState('failed');\n  }, []);\n\n  // ─────────────────────────────────────────────────\n  // Handle camera permission denied in WebView\n  // ─────────────────────────────────────────────────\n\n  const handleCameraPermissionDenied = useCallback(() => {\n    Alert.alert(\n      'Camera Access Required',\n      'BeerBot needs camera access for age verification. Please enable it in your device settings.',\n      [\n        { text: 'Cancel', style: 'cancel' },\n        {\n          text: 'Open Settings',\n          onPress: () => Linking.openSettings(),\n        },\n      ],\n    );\n    setScreenState('explanation');\n  }, []);\n\n  // ─────────────────────────────────────────────────\n  // Render: Checking State\n  // ─────────────────────────────────────────────────\n\n  if (screenState === 'checking') {\n    return (\n      <View\n        className=\"flex-1 bg-dark items-center justify-center\"\n        style={{ paddingTop: insets.top }}\n      >\n        <ActivityIndicator color=\"#f59e0b\" size=\"large\" />\n        <Text className=\"text-white/40 text-sm mt-4\">\n          Checking verification status...\n        </Text>\n      </View>\n    );\n  }\n\n  // ─────────────────────────────────────────────────\n  // Render: Veriff WebView\n  // ─────────────────────────────────────────────────\n\n  if (screenState === 'verifying' && sessionUrl) {\n    return (\n      <View className=\"flex-1 bg-dark\" style={{ paddingTop: insets.top }}>\n        {/* Header with cancel */}\n        <View className=\"px-6 py-3 flex-row items-center justify-between border-b border-dark-600\">\n          <Text className=\"text-lg font-semibold text-white\">\n            Age Verification\n          </Text>\n          <Pressable\n            onPress={() => {\n              Alert.alert(\n                'Cancel Verification?',\n                'Your progress will be lost.',\n                [\n                  { text: 'Continue Verifying', style: 'cancel' },\n                  {\n                    text: 'Cancel',\n                    style: 'destructive',\n                    onPress: () => setScreenState('explanation'),\n                  },\n                ],\n              );\n            }}\n            hitSlop={16}\n            className=\"active:opacity-60\"\n          >\n            <Text className=\"text-brand text-base\">Cancel</Text>\n          </Pressable>\n        </View>\n\n        <WebView\n          ref={webviewRef}\n          source={{ uri: sessionUrl }}\n          style={{ flex: 1 }}\n          javaScriptEnabled\n          domStorageEnabled\n          mediaPlaybackRequiresUserAction={false}\n          allowsInlineMediaPlayback\n          mediaCapturePermissionGrantType=\"grant\"\n          onNavigationStateChange={handleNavigationChange}\n          onError={handleWebViewError}\n          onHttpError={(syntheticEvent) => {\n            const { statusCode } = syntheticEvent.nativeEvent;\n            if (statusCode >= 400) {\n              handleWebViewError();\n            }\n          }}\n          startInLoadingState\n          renderLoading={() => (\n            <View className=\"absolute inset-0 bg-dark items-center justify-center\">\n              <ActivityIndicator color=\"#f59e0b\" size=\"large\" />\n              <Text className=\"text-white/40 text-sm mt-4\">\n                Loading verification...\n              </Text>\n            </View>\n          )}\n          {...(Platform.OS === 'android' && {\n            androidLayerType: 'hardware',\n            allowFileAccess: true,\n          })}\n        />\n      </View>\n    );\n  }\n\n  // ─────────────────────────────────────────────────\n  // Render: Processing State\n  // ─────────────────────────────────────────────────\n\n  if (screenState === 'processing' || screenState === 'success') {\n    return (\n      <View\n        className=\"flex-1 bg-dark items-center justify-center px-8\"\n        style={{ paddingTop: insets.top }}\n      >\n        {screenState === 'processing' ? (\n       