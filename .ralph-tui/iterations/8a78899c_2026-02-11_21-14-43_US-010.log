{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01QENrnU7DqXpTZ6UaQDV8dC","type":"tool_result","content":"     1→-- US-005: Atomic order creation RPC function\n     2→-- Validates inventory + creates order + decrements oz_remaining in a single transaction\n     3→\n     4→CREATE OR REPLACE FUNCTION create_order_atomic(\n     5→  p_user_id    uuid,\n     6→  p_tap_id     uuid,\n     7→  p_quantity   integer DEFAULT 1,\n     8→  p_expires_minutes integer DEFAULT 15\n     9→)\n    10→RETURNS jsonb\n    11→LANGUAGE plpgsql\n    12→SECURITY DEFINER\n    13→AS $$\n    14→DECLARE\n    15→  v_tap        RECORD;\n    16→  v_venue      RECORD;\n    17→  v_user       RECORD;\n    18→  v_pricing    RECORD;\n    19→  v_pour_size  decimal;\n    20→  v_total_oz   decimal;\n    21→  v_unit_price decimal;\n    22→  v_total_amt  decimal;\n    23→  v_order_id   uuid;\n    24→  v_expires_at timestamptz;\n    25→BEGIN\n    26→  -- Lock the tap row to prevent concurrent inventory modifications\n    27→  SELECT t.id, t.venue_id, t.beer_id, t.status, t.oz_remaining,\n    28→         t.low_threshold_oz, t.temp_ok\n    29→  INTO v_tap\n    30→  FROM taps t\n    31→  WHERE t.id = p_tap_id\n    32→  FOR UPDATE;\n    33→\n    34→  IF NOT FOUND THEN\n    35→    RETURN jsonb_build_object('error', 'Tap not found', 'code', 'TAP_NOT_FOUND');\n    36→  END IF;\n    37→\n    38→  -- Validate tap is active\n    39→  IF v_tap.status != 'active' THEN\n    40→    RETURN jsonb_build_object('error', 'Tap is not active', 'code', 'TAP_INACTIVE');\n    41→  END IF;\n    42→\n    43→  -- Validate temperature is OK\n    44→  IF v_tap.temp_ok = false THEN\n    45→    RETURN jsonb_build_object('error', 'Beer is not at proper temperature', 'code', 'TEMP_NOT_OK');\n    46→  END IF;\n    47→\n    48→  -- Validate venue is active and mobile ordering enabled\n    49→  SELECT v.id, v.is_active, v.mobile_ordering_enabled\n    50→  INTO v_venue\n    51→  FROM venues v\n    52→  WHERE v.id = v_tap.venue_id;\n    53→\n    54→  IF NOT FOUND THEN\n    55→    RETURN jsonb_build_object('error', 'Venue not found', 'code', 'VENUE_NOT_FOUND');\n    56→  END IF;\n    57→\n    58→  IF v_venue.is_active = false THEN\n    59→    RETURN jsonb_build_object('error', 'Venue is not active', 'code', 'VENUE_INACTIVE');\n    60→  END IF;\n    61→\n    62→  IF v_venue.mobile_ordering_enabled = false THEN\n    63→    RETURN jsonb_build_object('error', 'Mobile ordering is not enabled at this venue', 'code', 'MOBILE_ORDERING_DISABLED');\n    64→  END IF;\n    65→\n    66→  -- Validate user is age verified\n    67→  SELECT u.id, u.age_verified\n    68→  INTO v_user\n    69→  FROM users u\n    70→  WHERE u.id = p_user_id;\n    71→\n    72→  IF NOT FOUND THEN\n    73→    RETURN jsonb_build_object('error', 'User not found', 'code', 'USER_NOT_FOUND');\n    74→  END IF;\n    75→\n    76→  IF v_user.age_verified = false THEN\n    77→    RETURN jsonb_build_object('error', 'User has not completed age verification', 'code', 'AGE_NOT_VERIFIED');\n    78→  END IF;\n    79→\n    80→  -- Get pricing for this tap\n    81→  SELECT tp.price_12oz, tp.pour_size_oz, tp.currency\n    82→  INTO v_pricing\n    83→  FROM tap_pricing tp\n    84→  WHERE tp.tap_id = p_tap_id\n    85→  LIMIT 1;\n    86→\n    87→  IF NOT FOUND THEN\n    88→    RETURN jsonb_build_object('error', 'No pricing found for this tap', 'code', 'NO_PRICING');\n    89→  END IF;\n    90→\n    91→  v_pour_size := v_pricing.pour_size_oz;\n    92→  v_total_oz := v_pour_size * p_quantity;\n    93→\n    94→  -- Validate sufficient inventory (must be above low threshold after pour)\n    95→  IF v_tap.oz_remaining <= v_tap.low_threshold_oz THEN\n    96→    RETURN jsonb_build_object('error', 'Beer is running low and ordering is temporarily unavailable', 'code', 'INVENTORY_LOW');\n    97→  END IF;\n    98→\n    99→  IF v_tap.oz_remaining - v_total_oz < 0 THEN\n   100→    RETURN jsonb_build_object('error', 'Insufficient inventory for this order', 'code', 'INSUFFICIENT_INVENTORY');\n   101→  END IF;\n   102→\n   103→  -- Calculate pricing\n   104→  v_unit_price := v_pricing.price_12oz;\n   105→  v_total_amt := v_unit_price * p_quantity;\n   106→  v_expires_at := now() + (p_expires_minutes || ' minutes')::interval;\n   107→\n   108→  -- Decrement inventory atomically\n   109→  UPDATE taps\n   110→  SET oz_remaining = oz_remaining - v_total_oz\n   111→  WHERE id = p_tap_id;\n   112→\n   113→  -- Create the order\n   114→  INSERT INTO orders (\n   115→    user_id, venue_id, tap_id, beer_id,\n   116→    quantity, pour_size_oz, unit_price, total_amount, currency,\n   117→    status, expires_at\n   118→  )\n   119→  VALUES (\n   120→    p_user_id, v_tap.venue_id, p_tap_id, v_tap.beer_id,\n   121→    p_quantity, v_pour_size, v_unit_price, v_total_amt, v_pricing.currency,\n   122→    'pending_payment', v_expires_at\n   123→  )\n   124→  RETURNING id INTO v_order_id;\n   125→\n   126→  -- Log 'created' event in order_events\n   127→  INSERT INTO order_events (order_id, event_type, metadata)\n   128→  VALUES (\n   129→    v_order_id,\n   130→    'created',\n   131→    jsonb_build_object(\n   132→      'tap_id', p_tap_id,\n   133→      'quantity', p_quantity,\n   134→      'pour_size_oz', v_pour_size,\n   135→      'total_amount', v_total_amt,\n   136→      'expires_at', v_expires_at\n   137→    )\n   138→  );\n   139→\n   140→  -- Return the created order\n   141→  RETURN jsonb_build_object(\n   142→    'order_id', v_order_id,\n   143→    'venue_id', v_tap.venue_id,\n   144→    'tap_id', p_tap_id,\n   145→    'beer_id', v_tap.beer_id,\n   146→    'quantity', p_quantity,\n   147→    'pour_size_oz', v_pour_size,\n   148→    'unit_price', v_unit_price,\n   149→    'total_amount', v_total_amt,\n   150→    'currency', v_pricing.currency,\n   151→    'status', 'pending_payment',\n   152→    'expires_at', v_expires_at\n   153→  );\n   154→END;\n   155→$$;\n   156→"}]},"parent_tool_use_id":null,"session_id":"105bc883-5cda-4f2f-9ba4-aff9cfa15c01","uuid":"ab47f5df-f5a4-487b-a5ca-7cf550ee261e","tool_use_result":{"type":"text","file":{"filePath":"C:\\Users\\check\\beerbot\\beerbot-app\\supabase\\migrations\\20260211400000_create_order_rpc.sql","content":"-- US-005: Atomic order creation RPC function\n-- Validates inventory + creates order + decrements oz_remaining in a single transaction\n\nCREATE OR REPLACE FUNCTION create_order_atomic(\n  p_user_id    uuid,\n  p_tap_id     uuid,\n  p_quantity   integer DEFAULT 1,\n  p_expires_minutes integer DEFAULT 15\n)\nRETURNS jsonb\nLANGUAGE plpgsql\nSECURITY DEFINER\nAS $$\nDECLARE\n  v_tap        RECORD;\n  v_venue      RECORD;\n  v_user       RECORD;\n  v_pricing    RECORD;\n  v_pour_size  decimal;\n  v_total_oz   decimal;\n  v_unit_price decimal;\n  v_total_amt  decimal;\n  v_order_id   uuid;\n  v_expires_at timestamptz;\nBEGIN\n  -- Lock the tap row to prevent concurrent inventory modifications\n  SELECT t.id, t.venue_id, t.beer_id, t.status, t.oz_remaining,\n         t.low_threshold_oz, t.temp_ok\n  INTO v_tap\n  FROM taps t\n  WHERE t.id = p_tap_id\n  FOR UPDATE;\n\n  IF NOT FOUND THEN\n    RETURN jsonb_build_object('error', 'Tap not found', 'code', 'TAP_NOT_FOUND');\n  END IF;\n\n  -- Validate tap is active\n  IF v_tap.status != 'active' THEN\n    RETURN jsonb_build_object('error', 'Tap is not active', 'code', 'TAP_INACTIVE');\n  END IF;\n\n  -- Validate temperature is OK\n  IF v_tap.temp_ok = false THEN\n    RETURN jsonb_build_object('error', 'Beer is not at proper temperature', 'code', 'TEMP_NOT_OK');\n  END IF;\n\n  -- Validate venue is active and mobile ordering enabled\n  SELECT v.id, v.is_active, v.mobile_ordering_enabled\n  INTO v_venue\n  FROM venues v\n  WHERE v.id = v_tap.venue_id;\n\n  IF NOT FOUND THEN\n    RETURN jsonb_build_object('error', 'Venue not found', 'code', 'VENUE_NOT_FOUND');\n  END IF;\n\n  IF v_venue.is_active = false THEN\n    RETURN jsonb_build_object('error', 'Venue is not active', 'code', 'VENUE_INACTIVE');\n  END IF;\n\n  IF v_venue.mobile_ordering_enabled = false THEN\n    RETURN jsonb_build_object('error', 'Mobile ordering is not enabled at this venue', 'code', 'MOBILE_ORDERING_DISABLED');\n  END IF;\n\n  -- Validate user is age verified\n  SELECT u.id, u.age_verified\n  INTO v_user\n  FROM users u\n  WHERE u.id = p_user_id;\n\n  IF NOT FOUND THEN\n    RETURN jsonb_build_object('error', 'User not found', 'code', 'USER_NOT_FOUND');\n  END IF;\n\n  IF v_user.age_verified = false THEN\n    RETURN jsonb_build_object('error', 'User has not completed age verification', 'code', 'AGE_NOT_VERIFIED');\n  END IF;\n\n  -- Get pricing for this tap\n  SELECT tp.price_12oz, tp.pour_size_oz, tp.currency\n  INTO v_pricing\n  FROM tap_pricing tp\n  WHERE tp.tap_id = p_tap_id\n  LIMIT 1;\n\n  IF NOT FOUND THEN\n    RETURN jsonb_build_object('error', 'No pricing found for this tap', 'code', 'NO_PRICING');\n  END IF;\n\n  v_pour_size := v_pricing.pour_size_oz;\n  v_total_oz := v_pour_size * p_quantity;\n\n  -- Validate sufficient inventory (must be above low threshold after pour)\n  IF v_tap.oz_remaining <= v_tap.low_threshold_oz THEN\n    RETURN jsonb_build_object('error', 'Beer is running low and ordering is temporarily unavailable', 'code', 'INVENTORY_LOW');\n  END IF;\n\n  IF v_tap.oz_remaining - v_total_oz < 0 THEN\n    RETURN jsonb_build_object('error', 'Insufficient inventory for this order', 'code', 'INSUFFICIENT_INVENTORY');\n  END IF;\n\n  -- Calculate pricing\n  v_unit_price := v_pricing.price_12oz;\n  v_total_amt := v_unit_price * p_quantity;\n  v_expires_at := now() + (p_expires_minutes || ' minutes')::interval;\n\n  -- Decrement inventory atomically\n  UPDATE taps\n  SET oz_remaining = oz_remaining - v_total_oz\n  WHERE id = p_tap_id;\n\n  -- Create the order\n  INSERT INTO orders (\n    user_id, venue_id, tap_id, beer_id,\n    quantity, pour_size_oz, unit_price, total_amount, currency,\n    status, expires_at\n  )\n  VALUES (\n    p_user_id, v_tap.venue_id, p_tap_id, v_tap.beer_id,\n    p_quantity, v_pour_size, v_unit_price, v_total_amt, v_pricing.currency,\n    'pending_payment', v_expires_at\n  )\n  RETURNING id INTO v_order_id;\n\n  -- Log 'created' event in order_events\n  INSERT INTO order_events (order_id, event_type, metadata)\n  VALUES (\n    v_order_id,\n    'created',\n    jsonb_build_object(\n      'tap_id', p_tap_id,\n      'quantity', p_quantity,\n      'pour_size_oz', v_pour_size,\n      'total_amount', v_total_amt,\n      'expires_at', v_expires_at\n    )\n  );\n\n  -- Return the created order\n  RETURN jsonb_build_object(\n    'order_id', v_order_id,\n    'venue_id', v_tap.venue_id,\n    'tap_id', p_tap_id,\n    'beer_id', v_tap.beer_id,\n    'quantity', p_quantity,\n    'pour_size_oz', v_pour_size,\n    'unit_price', v_unit_price,\n    'total_amount', v_total_amt,\n    'currency', v_pricing.currency,\n    'status', 'pending_payment',\n    'expires_at', v_expires_at\n  );\nEND;\n$$;\n","numLines":156,"startLine":1,"totalLines":156}}}
