 302→        if (cancelled) return;\n   303→        if (tapData) setTapNumber(tapData.tap_number);\n   304→\n   305→        // Step 5: Fetch beer name\n   306→        const { data: beerData } = await supabase\n   307→          .from('beers')\n   308→          .select('name')\n   309→          .eq('id', orderData.beer_id)\n   310→          .single();\n   311→        if (cancelled) return;\n   312→        if (beerData) setBeerName(beerData.name);\n   313→\n   314→        // Step 6: Fetch venue name\n   315→        const venueData = await fetchVenue(orderData.venue_id);\n   316→        if (cancelled) return;\n   317→        if (venueData) setVenueName(venueData.name);\n   318→\n   319→        // Only set to 'ready' if not already in a terminal/pouring state\n   320→        if (\n   321→          orderData.status !== 'pouring' &&\n   322→          orderData.status !== 'completed' &&\n   323→          orderData.status !== 'expired'\n   324→        ) {\n   325→          setScreenState('ready');\n   326→        }\n   327→      } catch (err) {\n   328→        if (cancelled) return;\n   329→        setErrorMessage(\n   330→          err instanceof Error ? err.message : 'Failed to load order',\n   331→        );\n   332→        setScreenState('error');\n   333→      }\n   334→    }\n   335→\n   336→    load();\n   337→    return () => {\n   338→      cancelled = true;\n   339→    };\n   340→  }, [orderId]);\n   341→\n   342→  // Subscribe to realtime order status changes\n   343→  useEffect(() => {\n   344→    if (!orderId) return;\n   345→\n   346→    const channel = supabase\n   347→      .channel(`order-status-${orderId}`)\n   348→      .on(\n   349→        'postgres_changes',\n   350→        {\n   351→          event: 'UPDATE',\n   352→          schema: 'public',\n   353→          table: 'orders',\n   354→          filter: `id=eq.${orderId}`,\n   355→        },\n   356→        (payload) => {\n   357→          const updatedOrder = payload.new as Order;\n   358→          se